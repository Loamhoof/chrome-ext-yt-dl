{
    "name": "Youtube download",
    "version": "0",
    "manifest_version": 2,
    "background": {
        "scripts": ["main.js"]
    },
    "permissions": ["tabs", "storage"],
    "commands": {
        "download": {
            "description": "Send URL of the currently listened to YT song via HTTP GET request"
        }
    },
    "browser_action": {
        "default_popup": "popup.html"
    }
}
